{
  "manifest_version": 2,
  "name": "Custom Macros",
  "version": "1.0",
  "permissions": ["activeTab"],
  "commands": {
    "my_custom_command": {
      "suggested_key": {
        "default": "Ctrl+Shift+M",
        "mac": "Command+Shift+M"
      },
      "description": "Execute custom macro"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
